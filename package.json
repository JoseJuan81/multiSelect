{
  "name": "multiselect-db",
  "version": "0.1.1",
  "main": "dist/Multiselect.umd.js",
  "module": "dist/Multiselec.esm.js",
  "unpkg": "dist/Multiselec.min.js",
  "browser": {
    "./sfc": "src/components/MultiSelect.vue"
  },
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve",
    "prod": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "cypress run",
    "cy:open": "cypress open",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/MultiSelect.min.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/MultiSelect.esm.js",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/MultiSelect.umd.js"
  },
  "dependencies": {
    "core-js": "^3.4.4",
    "functionallibrary": "1.0.3",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "cypress": "3.8.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "rollup": "^1.29.0",
    "rollup-plugin-vue": "^5.1.5",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "bugs": {
    "url": "https://github.com/JoseJuan81/multiselect/issues"
  }
}
